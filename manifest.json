{
  "manifest_version": 3,
  "name": "Youtube Always Best Quality",
  "description": "Always play the best quality available on Youtube",
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/watch?v=*"
      ],
      "run_at": "document_end",
      "js": [
        "content.js"
      ]
    }
  ]
}